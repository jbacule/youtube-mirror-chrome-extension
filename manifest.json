{
    "name": "Youtube Mirror",
    "version": "1.0.0",
    "author": "shujo",
    "description": "Mirror videos in youtube",
    "permissions": [
      "storage",
      "tabs",
      "https://www.youtube.com/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://www.youtube.com/*"
        ],
        "js": ["js/jquery.js", "js/content.js"]
      }
    ],
    "browser_action": {
      "default_icon": {
        "16": "images/icon16.png",
        "24": "images/icon32.png",
        "32": "images/icon48.png"
      },
      "default_title": "Youtube Mirror",
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "manifest_version": 2
  }